<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent" >


	<RelativeLayout
		android:layout_width="match_parent"
		android:background="@drawable/nav_bg1"
		android:id="@+id/action"
		android:layout_height="80dp">
		<ImageView
			android:id="@+id/image_back"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="15dp"
			android:background="@drawable/back_bg"
			android:layout_alignTop="@+id/image_list"
			android:layout_alignParentStart="true"
			android:layout_marginStart="18dp" />
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:text="会员中心"
			android:textSize="25sp"
			android:layout_marginTop="32dp"
			android:textColor="#ffffff"
			android:id="@+id/textView13" />
		<CheckBox
			android:id="@+id/image_list"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true"
			android:button="@null"
			android:layout_marginRight="15dp"
			android:layout_marginTop="40dp"
			android:background="@drawable/share"
			/>

	</RelativeLayout>

	<ImageView
		android:id="@+id/imageView14"
		android:layout_width="80dp"
		android:layout_height="80dp"
		android:background="@drawable/avtar"
		android:layout_marginLeft="15dp"
		android:layout_marginTop="30dp"
		android:layout_below="@+id/action"
		android:layout_alignParentStart="true" />

	<TextView
		android:id="@+id/textView3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="梦。韩晓雨"
		android:layout_alignTop="@+id/imageView14"
		android:layout_toEndOf="@+id/imageView14" />

	<TextView
		android:id="@+id/textView11"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="全国健身房会员通用"
		android:layout_marginTop="12dp"
		android:layout_below="@+id/textView3"
		android:layout_toEndOf="@+id/imageView14" />

	<TextView
		android:id="@+id/textView14"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"

		android:text="非会员"
		android:layout_alignBaseline="@+id/textView3"
		android:layout_alignBottom="@+id/textView3"
		android:layout_toEndOf="@+id/textView3" />

	<TextView
		android:id="@+id/tv_type"
		android:layout_width="match_parent"
		android:layout_height="55dp"
		android:layout_marginTop="19dp"
		android:background="@drawable/bg_text_gradient"
		android:text="选择会员类型"
		android:padding="15dp"
		android:drawableRight="@drawable/more"
		android:gravity="center_vertical"
		android:layout_marginRight="10dp"
		android:textColor="@color/wheat"
		android:layout_below="@+id/imageView14"
		android:layout_alignStart="@+id/imageView14" />

	<TextView
		android:id="@+id/tv_protal"
		android:layout_marginTop="15dp"
		android:layout_width="match_parent"
		android:layout_height="55dp"
		android:background="@drawable/bg_text_gradient"
		android:text="查看会员协议"
		android:padding="15dp"
		android:drawableRight="@drawable/more"
		android:gravity="center_vertical"
		android:layout_marginRight="10dp"
		android:textColor="@color/wheat"
		android:layout_below="@+id/tv_type"
		android:layout_alignStart="@+id/tv_type" />

	<TextView
		android:id="@+id/tv_around"
		android:layout_width="match_parent"
		android:layout_height="55dp"
		android:background="@drawable/bg_text_gradient"
		android:text="查找附近门店"
		android:layout_marginTop="15dp"
		android:padding="15dp"
		android:drawableRight="@drawable/more"
		android:gravity="center_vertical"
		android:layout_marginRight="10dp"
		android:textColor="@color/wheat"
		android:layout_below="@+id/tv_protal"
		android:layout_alignStart="@+id/tv_protal" />














	<ImageButton
		android:id="@+id/button2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/rightnow"
		android:layout_alignTop="@+id/textView14"
		android:layout_alignEnd="@+id/tv_type" />

	<ImageView
		android:id="@+id/imageView15"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/tv_around"
		android:background="@drawable/huitequan" />

	<!--	<android.support.v4.view.ViewPager
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:id="@+id/vp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            >
        </android.support.v4.view.ViewPager>-->


</RelativeLayout>
